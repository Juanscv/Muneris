<div class="w-col w-col-4 w-clearfix", id="#friends">
  
  <div class="user-grid-item">
  <% for friendship in @friends %>

    <div class="usercircleprofile"><%= link_to image_tag(friendship.avatar.url, class: "usercircleprofile"), profile_path(friendship.id) %></div>
    
    <!--<img src="images/User%20circle.png" width="80" alt="52f3183c73df7f296b000533_User%20circle.png">-->
    <div class="user-grid-label">      
      <%= h link_to friendship.familyname, profile_path(friendship.id) %>
    </div>
  <% end %>


  </div>

</div>