
<div class="w-container dashboard-map">
<% if !@current_user.admin? %>
  	<div id="map" style="height: 300px;" data-widget-latlng="10.96421,-74.797043" data-widget-style="roadmap" data-widget-zoom="13"></div>
<% else %>
	<div id="map" style="height: 400px;" data-widget-latlng="10.96421,-74.797043" data-widget-style="roadmap" data-widget-zoom="13"></div>
<% end %>
</div>

<div>
	<div class="w-container">
  		<div class="w-row">
  			<div id="network" class="w-col w-col-3">
				<%= render partial: 'user/network' %>
			</div>
			<div id="feed" class="w-col w-col-5">
				<%= render partial: 'user/feed' %>
			</div>
			<div id="notifications" class="w-col w-col-3 notifications">
				<%= render partial: 'user/notifications' %>
			</div>
		</div>
	</div>
</div>

<script type="text/javascript">
	var App = App || {};

	App.map = { markers: <%= raw @markers.to_json %> };
</script>

<script src="//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry" type="text/javascript"></script>
<script src='//google-maps-utility-library-v3.googlecode.com/svn/tags/markerclustererplus/2.0.14/src/markerclusterer_packed.js' type='text/javascript'></script>
