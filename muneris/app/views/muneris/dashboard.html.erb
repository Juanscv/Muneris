<% if @user.admin? %> 
<div class="admin">
<% else %>
<div class="dashboard">
<% end %>
	<div class="w-container">
  		<div class="w-row">
  		<% if !@user.admin? %> 
  			<%= render partial: 'layouts/partials/sidebar' %>
  		<% else %>
  		<% end %>
		<div id="feed" class="w-col w-col-5">
			<%= render partial: 'user/feed' %>
		</div>
			<div id="network" class="w-col w-col-3">
			<%= render partial: 'user/network' %>
		</div>
		</div>
	</div>
</div>

<script src="//maps.google.com/maps/api/js?v=3.13&amp;sensor=false&amp;libraries=geometry" type="text/javascript"></script>
